.container-fluid
  .page-header
    %h1 All questions
  %ul.list-group
  - @questions.each do |question|
    %li.list-group-item
    %span.tag.tag-default.tag-pill.pull-xs-right= question.score
    %h4
      %a{href: question_path(question)}= question.title
    %small.text-muted= question.long_form_date
